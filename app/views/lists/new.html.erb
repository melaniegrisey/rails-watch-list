<div class="container">
  <div class="justify-content-center">
    <h1>Create a movie list</h1>
    <%= simple_form_for(@list) do |f| %>
      <%= f.input :name %>
      <%= f.submit %>
    <% end %>
  </div>
  <%= link_to lists_path do %>
    <i class="fa fa-history"></i> Back to my lists
  <% end %>
</div>
